    "viem": "^2.17.4",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.3.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.69",
