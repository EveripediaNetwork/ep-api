{
  "name": "ep-api",
  "private": true,
  "version": "0.0.1",
  "description": "Everipedia API",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build && yarn run copy:templates",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --loglevel warn",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "console": "node dist/src/console.js",
    "console:dev": "ts-node src/console.ts",
    "console-mail": "node dist/src/consoleMail.js",
    "console-mail:dev": "ts-node src/consoleMail.ts",
    "copy:templates": "mv ./dist/App/mailer/templates ./dist/src/App/mailer/ && rm -rf ./dist/App",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "yarn typeorm migration:generate -- -n",
    "typeorm:run": "yarn typeorm migration:run",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@everipedia/iq-utils": "^0.0.2",
    "@graphql-tools/utils": "^8.7.0",
    "@jest-mock/express": "^1.4.5",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.6",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.1.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/platform-fastify": "^8.4.5",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^8.0.3",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@pinata/sdk": "^1.1.24",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "7.17.2",
    "@sentry/tracing": "7.17.2",
    "@sentry/types": "7.17.2",
    "@types/nodemailer": "^6.4.6",
    "@types/remove-markdown": "^0.3.1",
    "apollo-server-express": "^3.6.3",
    "apollo-server-fastify": "^3.7.0",
    "apollo-server-plugin-base": "^3.6.3",
    "apollo-server-types": "^3.6.3",
    "cache-manager": "^3.6.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "defender-relay-client": "^1.18.0",
    "ethers": "^5.6.9",
    "express-rate-limit": "^6.6.0",
    "fast-diff": "^1.2.0",
    "get-ens": "^2.0.3",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "graphql-upload": "^13.0.0",
    "handlebars": "^4.7.7",
    "jest-mock": "^27.5.1",
    "mysql2": "^2.3.3",
    "nest-commander": "^2.4.0",
    "nodemailer": "^6.8.0",
    "pg": "^8.7.3",
    "pg-boss": "^8.1.1",
    "reflect-metadata": "^0.1.13",
    "remove-markdown": "^0.5.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "sharp": "^0.30.4",
    "slugify": "^1.6.5",
    "typeorm": "^0.2.44",
    "web3-token": "^0.2.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.3.1",
    "@types/cache-manager": "^3.4.3",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/sharp": "^0.30.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-testing-library": "^5.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.0",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "packageManager": "yarn@1.22.5"
}
