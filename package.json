    "@types/dompurify": "^3.0.2",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.1",
    "@types/localtunnel": "^2.0.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^17.0.21",
    "@types/react": "^19.0.8",
    "@types/sharp": "^0.30.2",
