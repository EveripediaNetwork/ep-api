    "cron": "^2.3.1",
    "decimal.js": "^10.4.3",
    "dompurify": "^3.0.2",
    "esm": "^3.2.25",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-rate-limit": "^6.6.0",
    "fast-diff": "^1.2.0",
    "get-ens": "^2.0.3",
    "got": "^12.5.3",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-upload": "^13.0.0",
    "handlebars": "^4.7.7",
