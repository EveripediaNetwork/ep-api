    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^29.6.1",
    "lint-staged": "^12.1.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.5.1",
    "react-email": "3.0.7",
    "rome": "12.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.2",
    "swc": "^1.0.11",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.6.2"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^18.17.0 || >=20.15.1",
    "pnpm": ">=9.0.0",
    "npm": "please-use-pnpm"
  },
  "packageManager": "pnpm@9.5.0"
}
