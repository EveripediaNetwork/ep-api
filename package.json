{
  "name": "ep-api",
  "private": true,
  "version": "0.0.1",
  "description": "IQ.WIKI API",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build && pnpm run copy:templates",
    "format": "pnpm rome format --write ./src",
    "check": "pnpm rome check ./src --apply",
    "start": "nest start",
    "dev": "nest start --watch",
    "build:swc": "npx swc --copy-files --out-dir dist/src src",
    "watch:swc": "npx swc --copy-files --out-dir dist/src -w src",
    "start:swc": "nodemon dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --trace-warnings dist/src/main.js ",
    "console": "node dist/src/console.js",
    "console:dev": "ts-node src/console.ts",
    "console-mail": "node dist/src/consoleMail.js",
    "console-mail:dev": "ts-node src/consoleMail.ts",
    "copy:templates": "mv ./dist/App/mailer/templates ./dist/src/App/mailer/ && rm -rf ./dist/App",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "pnpm typeorm migration:generate -- -n",
    "typeorm:run": "pnpm typeorm migration:run",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "email:dev": "email dev -p 3001"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@apollo/utils.keyvaluecache": "^2.1.0",
    "@elastic/ecs-winston-format": "^1.3.1",
    "@everipedia/iq-utils": "^0.4.3",
    "@everipedia/web3-signer": "^0.0.11",
    "@graphql-tools/utils": "^8.7.0",
    "@jest-mock/express": "^1.4.5",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.6",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.1.1",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/platform-fastify": "^8.4.5",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/serve-static": "^5.0.2",
    "@nestjs/typeorm": "^9.0.1",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@openzeppelin/defender-relay-client": "^1.54.1",
    "@pinata/sdk": "^2.1.0",
    "@react-email/components": "^0.0.33",
    "@react-email/render": "^1.0.5",
    "@sentry/minimal": "^6.19.7",
    "@sentry/node": "^7.80.1",
    "@sentry/tracing": "^7.80.1",
    "@sentry/types": "^7.80.1",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/got": "^9.6.12",
    "@types/nodemailer": "^6.4.6",
    "@types/react-dom": "^19.0.4",
    "@types/remove-markdown": "^0.3.1",
    "apollo-server-express": "^3.13.0",
    "apollo-server-fastify": "^3.13.0",
    "arweave": "^1.15.1",
    "axios": "^1.3.1",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
